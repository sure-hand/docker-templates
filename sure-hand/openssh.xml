<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>OpenSSH Server</Name>
  <Description>Dockerized OpenSSH Server</Description>
  <Registry>https://hub.docker.com/r/surehand/openssh/</Registry>
  <Repository>surehand/openssh</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9022</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/disk1</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>r</Mode>
    </Volume>
  </Data>
  <Version>3</Version>
  <WebUI/>
  <Banner/>
  <Icon>https://github.com/sure-hand/docker-openssh/blob/master/ssh-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
