<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Kodi server</Name>
  <Description>Runs Kodi in headless mode with docker</Description>
  <Registry>https://hub.docker.com/r/celedhrim/kodi-server</Registry>
  <Repository>celedhrim/kodi-server</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9089</HostPort>
        <ContainerPort>8089</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9777</HostPort>
        <ContainerPort>9777</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
  </Data>
  <Version>1</Version>
  <WebUI/>
  <Banner/>
  <!-- <Icon></Icon> -->
  <ExtraParams></ExtraParams>
</Container>
